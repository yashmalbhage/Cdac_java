package Staff;

public class Employee extends Person {
	private int empid;
	private double salary;
	public Employee(String name, int d, int m, int y, int empid, double salary) {
		super(name, d, m, y);
		this.empid = empid;
		this.salary = salary;
	}
	public Employee() {
		super();
		// TODO Auto-generated constructor stub
	}

}
